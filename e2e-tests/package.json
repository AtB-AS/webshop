{
  "private": true,
  "name": "cypress-e2e-tests",
  "description": "Cypress e2e-tests",
  "scripts": {
    "cypress:open": "cypress open -d",
    "cypress:run": "cypress run",
    "cypress:run:report": "cypress run --reporter mocha-multi-reporters --reporter-options configFile=cypress-reporter-config.json --config video=true --spec \"cypress/integration/tests/onboarding.e2e_spec.js\"",
    "cypress:html-testreport": "node cypress/scripts/combineTestReports.js && node_modules/.bin/marge cypress/results/cypressTestReport.json --code false --reportFilename cypressTestReport --reportDir ./cypress/results"
  },
  "devDependencies": {
    "axe-core": "4.2.2",
    "cypress": "7.1.0",
    "cypress-axe": "0.12.2",
    "cypress-fail-fast": "3.0.0",
    "imap-simple": "5.1.0",
    "mailparser": "3.2.0",
    "mocha": "8.3.2",
    "mocha-multi-reporters": "1.5.1",
    "mocha-junit-reporter": "2.0.0",
    "mochawesome": "6.2.2",
    "mochawesome-report-generator": "5.2.0",
    "nodemailer": "6.6.2",
    "prettier": "2.3.2",
    "shelljs": "0.8.4"
  }
}
